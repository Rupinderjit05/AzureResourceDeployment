            "type": "string"
        },
        "privateLinkResource": {
            "type": "string"
        },
        "targetSubResource": {
            "type": "array"
        },
        "requestMessage": {
            "type": "string"
        },
        "subnet": {
            "type": "string"
        },
        "virtualNetworkId": {
            "type": "string"
        },
        "virtualNetworkResourceGroup": {
            "type": "string"
        },
        "subnetDeploymentName": {
            "type": "string"
        }
    },
    "resources": [
        {
            "location": "[parameters('location')]",
            "name": "[parameters('privateEndpointName')]",
            "type": "Microsoft.Network/privateEndpoints",
